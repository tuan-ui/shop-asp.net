@model Web.Models.PageViewModel
@{
    ViewBag.Title = @Model.Name;
    ViewBag.MetaKeyword = @Model.Name;
    ViewBag.MetaDescription = Model.Name;
    Layout = "~/Views/Shared/Layout.cshtml";
}


<div class="container">
    <div class=""> 
        <h1>@Model.Name</h1>
        @Html.Raw(Model.Content)
    </div>
</div>




