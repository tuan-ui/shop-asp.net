<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta name="description" content="@ViewBag.MetaKeyword">
    <meta name="keywords" content="@ViewBag.MetaDescription">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="~/UploadedFiles/files/icon.jpg" />
    <link href="~/Assets/login/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Assets/login/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Assets/login/vendor/animate/animate.css" rel="stylesheet" />
    <link href="~/Assets/login/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" />
    <link href="~/Assets/login/vendor/select2/select2.min.css" rel="stylesheet" />
    <link href="~/Assets/login/css/main.css" rel="stylesheet" />
    <link href="~/Assets/login/css/util.css" rel="stylesheet" />

</head>

<body>
    @RenderBody()

    <script src="~/Assets/login/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/Assets/login/vendor/bootstrap/js/popper.min.js"></script>
    <script src="~/Assets/login/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Assets/login/vendor/select2/select2.min.js"></script>
    <script src="~/Assets/login/vendor/tilt/tilt.jquery.min.js"></script>

    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>

    <script src="~/Assets/login/js/main.js"></script>
</body>
</html>